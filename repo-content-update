from github import Github

# Replace with your GitHub access token
access_token = 'YOUR_ACCESS_TOKEN'

# Replace with your repository name and file details
repo_name = 'your_username/your_repository'
file_path = 'path/to/your/file.txt'
commit_message = 'Update file contents'
new_contents = 'This is the new content of the file.'

# Authenticate to GitHub
g = Github(access_token)

# Get the repository
repo = g.get_repo(repo_name)

# Get the file content
file = repo.get_contents(file_path)

# Update the file content
repo.update_file(file.path, commit_message, new_contents, file.sha)

print(f"File {file_path} updated successfully.")
